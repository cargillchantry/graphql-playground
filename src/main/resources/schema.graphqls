type Query {
    exercises(exerciseType: ExerciseType): [Exercise!]!
}

type Mutation {
    createExercise(exerciseType: ExerciseType, duration: String): Exercise
}

type Exercise {
    id: ID!
    exerciseType: ExerciseType!
    duration: String!
}

enum ExerciseType {
    RUN
    WALK
    HIKE
    SWIM
    BIKE
}