type Query {
    exercises(exerciseType: ExerciseType, duration: Duration): [Exercise!]!
}

type Mutation {
    createExercise(exerciseType: ExerciseType, duration: Duration): Exercise
}

type Exercise {
    id: ID!
    exerciseType: ExerciseType!
    duration: Duration!
}

enum ExerciseType {
    RUN
    WALK
    HIKE
    SWIM
    BIKE
}

scalar Duration